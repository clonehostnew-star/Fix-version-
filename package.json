{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "./node_modules/.bin/next dev --port ${PORT:-3000}",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "node scripts/start.js",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "postinstall": "node scripts/postinstall-check.js || true",
    "diagnose:runtime": "node -e \"console.log(JSON.stringify({ node: process.version, next: require('next/package.json').version, aiAvailable: (function(){ try { require.resolve('genkit'); require.resolve('@genkit-ai/googleai'); return true;} catch(e){ return false; } })()}, null, 2))\""
  },
  "engines": {
    "node": "20"
  },
